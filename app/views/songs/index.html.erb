<% @songs.each do |song| %>
  <%= song.title %>
  <br>
  <%= link_to "View Song Info", song_path(song) %>
  <br>
  <%= link_to "Edit Song Info", edit_song_path(song) %>
  <br>
  <%= link_to "Delete Song Record", song_path(song), method: "delete" %>
  <hr>
<% end %>
